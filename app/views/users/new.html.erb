<h3>Signup Here</h3>
<%= form_for(@user) do |f| %>
	 <%= f.label :username %>
		<%= f.text_field :username %>

		<%= f.label :email %>
		<%= f.email_field :email %>

		<%= f.label :password %>
		<%= f.password_field :password %>
		
		<%= f.submit "Create my account", class: "btn btn-primary" %>
<% end %>